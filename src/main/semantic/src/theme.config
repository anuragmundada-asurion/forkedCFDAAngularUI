/*******************************
        Theme Selection
*******************************/

/* To override a theme for an individual element
   specify theme name below
*/

/* Global */
@site       : '18f';
@reset      : '18f';

/* Elements */
@button     : '18f';
@container  : '18f';
@divider    : 'default';
@flag       : 'default';
@header     : '18f';
@icon       : 'default';
@image      : 'default';
@input      : '18f';
@label      : '18f';
@list       : '18f';
@loader     : 'default';
@rail       : 'default';
@reveal     : 'default';
@segment    : '18f';
@step       : 'default';

/* Collections */
@breadcrumb : 'default';
@form       : '18f';
@grid       : '18f';
@menu       : '18f';
@message    : 'default';
@table      : 'default';

/* Modules */
@accordion  : '18f';
@checkbox   : '18f';
@dimmer     : 'default';
@dropdown   : '18f';
@embed      : 'default';
@modal      : 'default';
@nag        : 'default';
@popup      : '18f';
@progress   : 'default';
@rating     : 'default';
@search     : '18f';
@shape      : 'default';
@sidebar    : 'default';
@sticky     : 'default';
@tab        : 'default';
@transition : 'default';

/* Views */
@ad         : 'default';
@card       : 'default';
@comment    : 'default';
@feed       : 'default';
@item       : '18f';
@statistic  : '18f';

/*******************************
            Folders
*******************************/

/* Path to theme packages */
@themesFolder : 'themes';

/* Path to site override folder */
@siteFolder   : 'site/';


/*******************************
         Import Theme
*******************************/

@import "theme.less";

/* End Config */
