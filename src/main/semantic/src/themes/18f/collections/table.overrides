/*-------------------
       Colors
--------------------*/


/* Dark Blue */
.ui.darkblue.table thead th{
  color: @white;
  background-color: @darkBlue;
  line-height: 1.2em;
}

/* Dark Blue Sorting */
.ui.darkblue.table thead th.sorting_asc,
.ui.darkblue.table thead th.sorting_desc{
  background-color: @blue !important;
}
.ui.darkblue.table td.sorting_1{
  background-color: #fffded;
}

